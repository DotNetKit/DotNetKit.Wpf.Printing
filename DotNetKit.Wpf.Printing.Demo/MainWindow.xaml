<Window
    x:Class="DotNetKit.Wpf.Printing.Demo.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:DotNetKit.Wpf.Printing.Demo"
    xmlns:helloWorldSample="clr-namespace:DotNetKit.Wpf.Printing.Demo.Samples.HelloWorldSample"
    xmlns:multipageReportSample="clr-namespace:DotNetKit.Wpf.Printing.Demo.Samples.MultipageReportSample"
    xmlns:asynchronousSample="clr-namespace:DotNetKit.Wpf.Printing.Demo.Samples.AsynchronousSample"
    mc:Ignorable="d"
    Title="MainWindow"
    Width="1024"
    Height="768"
    >
    <TabControl>
        <TabItem Header="Hello World">
            <helloWorldSample:SampleControl />
        </TabItem>

        <TabItem Header="Multipage">
            <multipageReportSample:SampleControl />
        </TabItem>

        <TabItem Header="Async">
            <asynchronousSample:SampleControl />
        </TabItem>
    </TabControl>
</Window>
